multichain-util create reconhecer -'anyone-can-connect'=true -'anyone-can-send'=true -'anyone-can-receive'=true -'anyone-can-issue'=true -'default-rpc-port'=5555 -'default-network-port'=4000
multichaind reconhecer -daemon -rpcuser='usuario' -rpcpassword='senha'
echo "Blockchain 'reconhecer' criado com sucesso"
